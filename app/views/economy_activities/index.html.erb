<p id="notice"><%= notice %></p>

<h1>Economy Activities</h1>

<table>
  <thead>
    <tr>
      <th>CIIU</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @economy_activities.each do |economy_activity| %>
      <tr>
        <td><%= economy_activity.ciiu %></td>
        <td><%= economy_activity.name %></td>
        <td><%= link_to 'Show', economy_activity %></td>
        <td><%= link_to 'Edit', edit_economy_activity_path(economy_activity) %></td>
        <td><%= link_to 'Destroy', economy_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Economy Activity', new_economy_activity_path %>
