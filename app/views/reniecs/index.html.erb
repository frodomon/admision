<% provide(:title, "AELUCOOP - AdmisiÃ³n de Socios") %>
<p id="notice"><%= notice %></p>
  <div class="wrapper wrapper-content animated fadeInRight no-padding">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h1>Reniec</h1>
        </div>
        <div class="ibox-content">
          <div class='table-responsive'>
            <table class="table table-stripped table-bordered table-hover datatable">
              <thead>
                <tr>
                  <th>Dni</th>
                  <th>Last name</th>
                  <th>Second last name</th>
                  <th>Name</th>
                  <th></th>
                  <th></th>
                  <th></th>
                </tr>
              </thead>

              <tbody>
                <% @reniecs.each do |reniec| %>
                  <tr>
                    <td><%= reniec.doi %></td>
                    <td><%= reniec.last_name %></td>
                    <td><%= reniec.second_last_name %></td>
                    <td><%= reniec.name %></td>
                    <td><%= link_to 'Show', reniec %></td>
                    <td><%= link_to 'Edit', edit_reniec_path(reniec) %></td>
                    <td><%= link_to 'Destroy', reniec, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
<br>

<%= link_to 'Nuevo Reniec', new_reniec_path, class: 'btn btn-wm btn-success' %>
<% content_for :javascript do %>
  <script type="text/javascript">
    $(document).ready(function() {
      load_table_controls('datatable','Reniec');
    });
  </script>
<% end %>
