<p id="notice"><%= notice %></p>

<h1>Alianzas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alianzas.each do |alianza| %>
      <tr>
        <td><%= alianza.nombre %></td>
        <td><%= link_to 'Show', alianza %></td>
        <td><%= link_to 'Edit', edit_alianza_path(alianza) %></td>
        <td><%= link_to 'Destroy', alianza, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alianza', new_alianza_path %>
